  </div>
<% else %>

<div class="center jumbotron">
  <h1>Welcome to Finyl Vinal</h1>
  <p>More stuff</p>
  <p>More stuff</p>
  <%= link_to "Sign up, yo!", signup_path, class: "btn btn-lg btn-primary" %>
</div>
<% end %>
